<?xml version="1.0"?>
<keymap>
    <map key="KEY_RIGHT" action="next" />
    <map key="KEY_LEFT" action="prev" />
    <map key="KEY_S" action="stop" />
    <map key="KEY_SPACE" action="play" />
    <map key="KEY_UP" action="vol_up" />
    <map key="KEY_DOWN" action="vol_down" />
    <map key="KEY_Q" action="quit" />
    <map key="KEY_J" action="ff" />
    <map key="KEY_K" action="bb" />
</keymap>
